@model JSDstr.ViewObjects.ClusterizationResultViewObject[]
@{
    ViewBag.Title = @Resources.Title_Results;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="results">
    <h3>Results of clusterization</h3>
    <br/>
    <p>Points on the map present earthquakes with different intensity (size of the point).</p>
    <p>Points in the same cluster is filled with one color (red - strongest, brown - strong, yellow - medium, green - weak).</pmuted>
    <div id="map"></div>
</div>

@section script
{
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script type="text/javascript">
        jsd.clusterizationResults.drawClusters(JSON.parse('@Html.Raw(@Json.Encode(Model))'));
    </script>
}
